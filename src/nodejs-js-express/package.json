    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  }
